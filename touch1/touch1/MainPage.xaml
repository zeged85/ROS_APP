             xmlns:local="clr-namespace:touch1"
             x:Class="touch1.MainPage"
             xmlns:ef="clr-namespace:AiForms.Effects;assembly=AiForms.Effects">


    <StackLayout HeightRequest="300" ef:AddTouch.On="true" >
        <!-- Place new controls here -->
